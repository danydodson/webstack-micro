{
  "name": "passportjs-auth",
  "description": "centralized authentication for webstack-micro",
  "version": "0.1.0",
  "scripts": {
    "start": "run-s copy:shared serve",
    "serve": "node dist/server.js",
    "build": "run-s copy:shared build:ts",
    "build:ts": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{ts}\" --quiet --fix",
    "dev": "npm run copy:shared && run-p watch:ts watch:node",
    "watch:node": "nodemon dist/server.js",
    "watch:ts": "tsc -w",
    "test": "jest --forceExit --coverage --verbose",
    "watch:test": "npm run test -- --watchAll",
    "debug": "run-s build debug:watch",
    "debug:serve": "nodemon --inspect dist/server.js",
    "debug:watch": "run-p watch:ts debug:serve",
    "copy:shared": "./bin/copy-shared-constants.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^3.4.2",
    "crypto-random-string": "^3.0.1",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.16.2",
    "express-validator": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "lusca": "^1.6.1",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.2.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "^0.0.36",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.0",
    "@types/express-flash": "0.0.0",
    "@types/express-session": "^1.15.13",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/lusca": "^1.6.0",
    "@types/node": "^12.0.10",
    "@types/node-fetch": "^2.5.0",
    "@types/nodemailer": "^6.2.0",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-github": "^1.1.5",
    "@types/passport-local": "^1.0.33",
    "@types/passport-google-oauth20": "^2.0.2",
    "@types/redis": "^2.8.13",
    "@types/request": "^2.48.1",
    "@types/supertest": "^2.0.7",
    "@types/winston": "^2.3.9",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "chai": "^4.2.0",
    "eslint": "^5.0.0",
    "fs-extra": "^8.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "license": "MIT"
}
